{
  "AppSettings": {
    "Token": "super secret key",
    "ProjectName":"CMS",
    "ReportPics":"\\assets\\ReportPics\\"
  },
  "ConnectionStrings": {
    "DKSConnection": "Server=10.4.0.39;Database=SHCDEV3;MultipleActiveResultSets=true;User Id=sa;Password=shc@1234;connect timeout=60;",
    "CMSConnection": "Server=10.4.0.41;Database=SHC_ADMS;MultipleActiveResultSets=true;User Id=sa;Password=shc@123;connect timeout=60;"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "Serilog": 
  {
    "Using": [],
    "MinimumLevel": {
      "Default": "Information",
      "Override": 
      {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "Console"
      },
      {
        "Name": "File",
        "Args": {
          "path": "D:\\Logs\\log.txt",
          "rollingInterval": "Day",
          "outputTemplate": "{Timestamp:HH:mm:ss} **Level :{Level:u3}** {Message:lj} {NewLine:1} {Exception:1}"
        }
      }
    ],
    "Enrich": [
      "FromLogContext",
      "WithMachineName",
      "WithProcessId",
      "WithThreadId"
    ],
    "Properties": {
      "ApplicationName": "Serilog.WebApplication"
    }
  },
  "LogicSettings": {
    "DateExcelFormat": "MM/dd/yyyy",
    "MinDate":"1911/01/01",
    "MaxDate":"2099/12/31",

    
    "NoNumber":0,
    "YesNumber":1,
    "NullCodeNumber":0
  },
  "AllowedHosts": "*"
}
